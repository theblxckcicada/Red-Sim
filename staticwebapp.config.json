{

  "navigationFallback": {
    "rewrite": "/index.html",

    "exclude": ["/images/*.{png,jpg,gif}", "/css/*","/*.{png,jpg,gif}"]
  },

  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",

    "Access-Control-Allow-Methods": "GET, PUT, POST, DELETE, OPTIONS",

    "Access-Control-Allow-Headers": "Content-Type, x-requested-with"
  }
}
